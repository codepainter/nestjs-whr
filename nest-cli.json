{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/project/src",
  "monorepo": true,
  "root": "apps/project",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/project/tsconfig.app.json"
  },
  "projects": {
    "project": {
      "type": "application",
      "root": "apps/project",
      "entryFile": "main",
      "sourceRoot": "apps/project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/project/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    }
  }
}
